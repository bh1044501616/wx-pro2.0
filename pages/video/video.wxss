/* 下载页面 */
.title{
	margin:auto;
	width: 20%;
	border-bottom-style: solid;
	border-bottom-color: #046ebd;
	border-bottom-width: 3px;
	margin-top: 5%;
}
.line_title{
	width: 95%;
	margin:auto;
	border-bottom-style: solid;
	border-bottom-color: #1c93ec;
}
#downloadPage .downloading{
	border-radius: 5px;
	background-color: #fff;
	width: 96%;
	display: block;
	margin: auto;
	margin-top: 4px;
}
#downloadPage .downloading>.title{
	display: flex;
	flex-direction: row;
	font-size: 10px;
}
#downloadPage .downloading>.title view{
	width: 50%;
	color: #555;
	padding: 5px;
}
#downloadPage .downloading .title .method{
	text-align: right;
	color:#1aac19;
}
#downloadPage .downloading>.content{
	display: flex;
	flex-direction:row;
	align-items: center;
}
#downloadPage .downloading>.content>.icon{
	width:15%;
}
#downloadPage .downloading>.content>.icon image{
	width: 100%;
	height: 100%;
	border-radius: 7px;
	margin-top: 3px;
}
#downloadPage .downloading>.content>.fileInfo{
	width: 55%;
	font-size:10px;
}
#downloadPage .downloading>.content>.button{
	width: 10%;
	text-align: right;
}
#downloadPage .downloading>.content>.button image{
	width:40%;
	height:40%;
}
#downloadPage .downloading>.content>.manage{
	width: 20%;
}
#downloadPage .downloading>.content>.manage view{
	width: 80%;
	font-size: 13px;
	border-radius: 10px;
	border: 1px solid #1aac19;
	text-align: center;
	color:#1aac19;
	margin: auto;
}
/* downloaded */
#downloadPage .downloaded{
	border-radius: 5px;
	background-color: #fff;
	width: 96%;
	display: block;
	margin: auto;
	margin-top: 5px;
}
#downloadPage .downloaded>.title{
	font-size: 10px;
	height: 10%;
	display: flex;
	flex-direction: row;
}
#downloadPage .downloaded>.title view{
	width: 50%;
	color: #555;
	padding: 5px;
}
#downloadPage .downloaded .title .method{
	text-align: right;
	color:#1aac19;
	float: right;
}
#downloadPage .downloaded>.content{
	display: flex;
	flex-direction:row;
	align-items: center;
/*	border-top: 0.1px  solid #777;*/
}
#downloadPage .downloaded>.content>.icon{
	width:15%;
}
#downloadPage .downloaded>.content>.icon image{
	width: 100%;
	height: 100%;
	border-radius: 7px;
	margin-top: 3px;
}
#downloadPage .downloaded>.content>.fileInfo{
	width: 55%;
	font-size:10px;
}
#downloadPage .downloaded>.content>.button{
	width: 10%;
	text-align: right;
}
#downloadPage .downloaded>.content>.button image{
	width:40%;
	height:40%;
	margin-top: 4px;
}
#downloadPage .downloaded>.content>.manage{
	width: 20%;
}
#downloadPage .downloaded>.content>.manage view{
	width: 80%;
	font-size: 13px;
	border-radius: 10px;
	border: 1px solid #1aac19;
	text-align: center;
	color:#1aac19;
	margin: auto;
}
/* 重新设定下载页面样式*/
#downloadPage .title{
	text-align: left;
	width: 100%;
	border-bottom: 0.5 solid #777;
}
#downloadPage .title view{
	display: inline;
	font-size: 12px;
}

/* 设置downloadPage*/
#downloadPage{
	width: 80%;
	height: 90%;
	position: fixed;
	left: -80%;
	top:0;
	background-color: #ddd;
	z-index: 9999;
	border-bottom-right-radius:10px;
	border-top-right-radius:10px;
	overflow-y: hidden;
}
/* 设置downloadPage开关*/
#downloadPageSwitch{
	position: fixed;
	width: 15%;
	height: 7%;
	z-index: 9999;
	top:90rpx;
	left: -15%;
}
#downloadPageSwitch image{
	width: 100%;
	height: 100%;
}
/* 为下载页面的每一个元素名字加 一条下划线*/
#downloadPage #fileName{
	border-bottom: 0.5px solid #ddd;
}

/* 购票咨询 */
#contact{
	width: 30%;
	height: 8%;
	position: fixed;
	right: -15%;
	bottom: 20px;
	z-index: 5000;
}
#contact image{
	width: 100%;
	height: 100%;
}



