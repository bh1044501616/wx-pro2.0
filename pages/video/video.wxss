/*更改下载页面样式*/
#downloadPage{
	height: 100%;
	left: -85%;
	border-radius: 0;
	background-color: #fff;
	box-shadow: 0px 0px 20px 1px #555;
}
#downloadPage .content{
	margin-bottom: 5px;
	border:0;
	box-shadow: 0;
	border-top:0;
	width: 97%;
	margin:auto;
	margin-top: 5px;
	border-radius: 3px;
}
#downloadPage .downloading{
	background-color: #eee;
	margin-top: 5px;
	padding-bottom: 5px;
}
#downloadPage .downloaded{
	background-color: #eee;
	padding-bottom: 5px;
}
#downloadPage .icon{
	width: 20%;
}
#downloadPage .button{
	width: 5%;
}
#downloadPage .button image{
	width: 50%;
}

/**没有更多信息提示*/
#meetingNumTip{
	width: 100%;
	height: 20px;
	text-align: center;
	color:#aaa;
	line-height: 20px;
	font-size: 12px;
	margin-bottom: 10px;
	margin-top: 20px;
	float: left;
}


/**下载文件所在目录提示*/
#showSavedFilePath{
	margin-top: 20px;
	width: 100%;
	font-size: 12px;
	color:#ccc;
	text-align: center;
}

/**信息获取提示*/
#serverInfo{
	width:100%;
	margin: auto;
	text-align: center;
	margin-top: 20px;
}

/**上部日期导航*/
#scrollNavi{
	width: 100%;
	height: 88rpx;
	display: inline-block;
	white-space: nowrap; 
	box-shadow: 0px 0px 10px 1px #aaa;
	position: fixed;
	top: 0;
	left: 0;
	background: #fff;
	font-size: 13px;
	z-index: 100;
}
/*占位用*/
#scrollNavi0{
	width: 100%;
	height: 88rpx;
	display: inline-block;
	white-space: nowrap; 
	z-index: 1;
}
#scrollNavi .item{
	width: 30%;
	height: 85rpx;
	line-height: 85rpx;
	vertical-align: center;
	text-align: center;
	display: inline-block;
	color:#aaa;
	font-weight: lighter;
}
#scrollNavi .selected-item{	
	width: 30%;
	height: 85rpx;
	line-height: 85rpx;
	vertical-align: center;
	text-align: center;
	display: inline-block;
	color:#000;
	font-weight: normal;
	border-bottom:5px solid #000;
}


