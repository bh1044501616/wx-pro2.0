page{
	width: 100%;
	height: 100%;
	background-color: #a2a19d;
}
#middle{
	width: 100%;
	height: 100%;
	position: relative;
}
/**导航*/
#navigator_words{
	width: 50%;
	height: 4%;
	display: flex;
	flex-direction: row;
	float: right;
	position: absolute;
	right: 0;
	top: 0;
	z-index: 10;
}
#navigator_words image{
	width: 40%;
	height: 100%;
	margin-right: 10px;
}
#navigator_line{
	width: 50%;
	height: 1%;
	position: absolute;
	top: 1%;
	right: 0;
	z-index: 10;
}
#navigator_line image{
	width: 30%;
	height: 100%;
}
#navigator_menu{
	position: absolute;
	top: 0;
	left: 0;
	width: 50%;
	height: 4%;
	display: flex;
	flex-direction: row;
	z-index: 10;
}
#navigator_menu image{
	width: 13%;
	height: 80%;
	padding: 4px;
}
/*为了遮盖住menu*/
#navigator_menu,#navigator_words{
	/*background-color: white;*/
}
/**菜单*/
#menu{
	width: 40%;
	height: 100%;
	text-align: center;
	font-size: 12px;
	background-color: #fcfcfc;
	color: #555;
	z-index: 9;
	position: fixed;
	left: -40%;
	box-shadow: 0px 0px 7px 7px #f8f8f8 inset;
}
#menu view{
	border-bottom: 0.5px solid #eee;
	padding-top:10px;
	padding-bottom: 10px;
	padding-left: 10px;
	display: flex;
	flex-direction: row;
	width: 80%;
	height: 3%;
	text-align: center;
}
#menu view image{
	width: 10%;
	height: 100%;
}
/*菜单上空出距离*/
#menu .block{
	width: 100%;
	height: 5%;
	border: 0;
	padding: 0;
}
/**轮播*/
#swiper{
	width: 100%;
	height: 50%;
	z-index: 9;
	overflow-y: hidden;
}
#swiper image{
	width: 100%;
	height: 100%;
}

/**主要内容*/
/*智联联盟简介*/
#zllm{
	width: 100%;
	margin-top: 10px;
	font-size: 10px;
}
#zllm image{
	width: 100%;
}
#zllm view{
	margin: auto;
}
#zllm .title{
	font-size: 20px;
	margin-top:10px;
	margin-bottom: 5px;
	text-align: left;
	height: 10%;
	border-bottom: 1px solid rgba(0,164,219,0.5);
}
#zllm .title .time{
	font-size: 10px;
	text-align: left;
	color:#999;
	vertical-align:bottom;
	height: 10%;
}
#zllm .words{
	padding-bottom: 5px;
	width: 90%;
	text-align: left;
	margin-top: 10px;
	line-height: 15px;
	font-size: 12px;
}
#contents{
	width: 100%;
	margin-top: 20px;
}
/*智联标委会*/
#zlbwh{
	width: 100%;
}
#zlbwh .title{
	width: 90%;
	margin: auto;
	font-size: 15px;
}
#zlbwh .title .time{
	color:#999;
	font-size: 10px;
}
#zlbwh .words{
	width: 90%;
	margin: auto;
	box-shadow: 0px 0px 5px 1px #888;
	padding: 5px;
	font-size: 12px;
	margin-top: 10px;
	line-height: 20px;
	margin-bottom: 20px;
}
#zlbwh .words .more_box{
	display: flex;
	flex-direction: row-reverse;
}
#zlbwh .more{
	border-radius: 10px;
	border:1px solid #09bb07;
	color:#09bb07;
	width: 20%;
	text-align: center;
	line-height: 15px;
	font-size: 10px;
	padding:2px;
}
/*成本度量标准应用*/
#contents1{
	width: 100%;
	background-color: #fff;
	border-bottom:0.5px solid #ddd;
	border-top:0.5px solid #ddd;
}
#contents1 .header{
	color:#888;
	font-size: 13px;
	border-bottom: 0.5px solid #ddd;
	text-align: left;
	width: 90%;
	margin:auto;
	line-height: 40px;
}
#contents1 .content{
	border-top: 0.5px solid #ddd;
	width: 90%;
	margin: auto;
}
#contents1 .content .brief{
	font-size: 15px;
	line-height: 30px;
}
#contents1 .content .words{
	font-size: 12px;
	color:#777;
	padding-bottom:5px;
}
#contents1 .footer{
	font-size: 13px;
	color:#586c94;
	width: 100%;
	margin: auto;
	line-height: 30px;
}
#contents1 .footer .box{
	width: 90%;
	margin: auto;
	position: relative;
	border-top: 0.5px solid #ddd;
}
#contents1 .footer .arrow{
	display: inline-block;   
    border-right: 1px solid #777; 
    border-bottom: 1px solid #777;  
    width: 8px; 
    height: 8px;  
    transform: rotate(-45deg); 
    right: 0;
    position: absolute;
    right:0px;
    top: 10px;
}
page{
