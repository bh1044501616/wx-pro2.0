/** 联系我们！！！*/
#contact{
	width: 94%;
	color:#999;
	margin: auto;
	font-size: 13px;
}
#contact .title{
	color:#333;
	font-size: 15px;
	line-height: 40px;
}
#contact .info{
	width: 100%;
	line-height: 20px;
}
#contact .info .detail{
	display: flex;
	flex-direction: row;
	line-height: 30px;
	color:black;
}
#contact .info .image{
	width: 12px;
	height: 12px;
	margin-right: 10px;
}
#contact .info .image image{
	width: 100%;
	height: 100%;
}
#contact .info text{
	overflow: visible;
}
/** 页尾*/
#pageFooter{
	width: 100%;
	background-color: #333;
	color: #999;
	margin-top: 30px;
}
#pageFooter .body{
	border-bottom: 0.5px solid #888;
	width: 90%;
	margin: auto;
	display: flex;
	flex-direction: row;
}
#pageFooter .body .title{
	font-size: 15px;
	color: #fff;
	line-height: 40px;
	width: 50%;
}
#pageFooter .body .content{
	display: flex;
	flex-direction: row-reverse;
	font-size: 11px;
	line-height: 40px;
	width: 50%;
}
#pageFooter .body .content view{
	text-align: right;
	padding-left: 5px;
}
#pageFooter .rights{
	width: 90%;
	line-height: 40px;
	margin: auto;
	font-size: 12px;
}


