  <view class="content_navi {{contentNaviSelected[index]==1?'selected':''}}" style="top: {{index*50}}rpx;" wx:for="{{content}}">
    {{item.title}}
  </view>
</view> -->


<view id="pageFooter">
  <view class="body">
    <view class="title">
      IQ alliance
    </view>
    <view class="content">
      <view class="joinzlbwh" bindtap="switchBar">
        加入智联标委会
      </view>
      <view class="joinprivi" bindtap="switchBar">
        会员申请
      </view>
    </view>
    </view>
    <view class="rights">
      © 2018 Creation All Rights Reserved
    </view>
  </view>
</view>




